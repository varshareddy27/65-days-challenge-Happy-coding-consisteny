public class Main
{	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int c=0;
		for(int i=1;i<=Math.sqrt(n);i++){
		    if(n%i==0){
		        System.out.println(i);
		        
		        if(n/i!=i){
		             System.out.println(n/i);
		        }}}}}
